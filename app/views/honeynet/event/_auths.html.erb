<div class="info">
	<div class="prompt">
		<div class="host"><%= event.host %>:</div><div class="pwd"><%= event.message[:pwd] %></div> [<%= event.message[:subject] %> on <%= event.message[:tty] %>]$
	</div>
	<div class="command">
		sudo <%= event.message[:command] %>
	</div>
</div>
<div class="metainfo">
	<% unless tags.empty? -%>
	<div class="debtags"> 
		<% tags.select { |t| event.debtag_list.member?(t) }.each do |t| -%>
		<div class='tag'><%= t %></div>
		<% end -%>
	</div>
	<% end -%>
	<% unless event.package_list.empty? -%>
	<div class="package">
		<% event.package_list.each do |t| -%>
		<div class='tag'><%= t %></div>
		<% end -%>
	</div>
	<% end -%>
	<div class="date">
		<%= event.observed_at.in_time_zone("UTC").strftime("%d/%b/%y %H:%M:%S") %>
	</div>
</div>
